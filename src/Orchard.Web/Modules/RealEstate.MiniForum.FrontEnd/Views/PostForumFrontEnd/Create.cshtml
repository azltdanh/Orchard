@{ 
    Layout.Title = T("Đăng bài viết mới").ToString(); 
}

@using (Html.BeginFormAntiForgeryPost(null, FormMethod.Post, new { enctype = "multipart/form-data", id = "formUpdateId" }))
{
    @Html.ValidationSummary()
    // Model is a Shape, calling Display() so that it is rendered using the most specific template for its Shape type
    @Display(Model);
                   
    <button value="submit.Save" name="submit.Save" type="submit" class="btn btn-warning">Đăng bài viết</button>
}