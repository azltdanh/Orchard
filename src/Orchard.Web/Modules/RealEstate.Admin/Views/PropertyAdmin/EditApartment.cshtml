@{ Layout.Title = T("Edit Property").ToString(); }

@using (Html.BeginFormAntiForgeryPost(null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{ 
    @Html.ValidationSummary() 
    <button value="submit.Save" name="submit.Save" type="submit" class="primaryAction">Save</button>
    // Model is a Shape, calling Display() so that it is rendered using the most specific template for its Shape type
    @Display(Model)
} 
