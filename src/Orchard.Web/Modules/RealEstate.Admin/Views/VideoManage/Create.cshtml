@{ Layout.Title = T("Thêm mới video").ToString(); }

@using (Html.BeginFormAntiForgeryPost(null, FormMethod.Post, new { enctype = "multipart/form-data", id = "form-video-management" }))
{
    @Html.ValidationSummary()
    // Model is a Shape, calling Display() so that it is rendered using the most specific template for its Shape type
    @Display(Model)
}
<script>
    $(function() {
        $('#form-video-management').submit(function() {
            $('#overlay').show();
        });
    })
</script>